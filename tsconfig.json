{
    "$schema": "https://json.schemastore.org/tsconfig",
    "_version": "22.0.0",
    "compilerOptions":
    {
        "target": "ES2022",
        "lib": ["ES2023"],
        "paths":
        {
            "@/*": ["./src/*"]
        },
        "module": "node16",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "node16"
    }
}